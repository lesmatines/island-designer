    "tslint": "^5.12.1",
    "tslint-microsoft-contrib": "^6.0.0",
    "gulp": "^4.0.0",
    "gulp-typescript": "^5.0.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "ts-loader": "^5.3.1",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "keywords": [
    "animal crossing",
    "new horizons",
    "island designer"
