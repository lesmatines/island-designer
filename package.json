{
  "name": "island-designer",
  "version": "0.0.1",
  "description": "A small tool for players designing their island in Animal Crossing - New Horizons",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "gulp",
    "dev": "webpack-dev-server --watch",
    "package": "gulp build-watch",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint-fix": "tslint -c tslint.json --fix 'src/**/*.ts'",
    "add-tag": "node scripts/tag.js --package ./package.json --changelog ./CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:lesmatines/island-designer.git"
  },
  "dependencies": {
    "three": "0.116.0"
  },
